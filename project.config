[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/ashishp-optimus
[access "refs/for/refs/*"]
	read = group user/ashishp-optimus
	push = group user/ashishp-optimus
	label-Code-Review = -2..+2 group user/ashishp-optimus
	label-Verified = -1..+1 group user/ashishp-optimus
	rebase = group user/ashishp-optimus
	removeReviewer = group user/ashishp-optimus
	submit = group user/ashishp-optimus
[access "refs/heads/*"]
	read = group user/ashishp-optimus
	create = group user/ashishp-optimus
	push = +force group user/ashishp-optimus
	pushMerge = group user/ashishp-optimus
[access "refs/tags/*"]
	push = +force group user/ashishp-optimus
	pushTag = group user/ashishp-optimus
	pushSignedTag = group user/ashishp-optimus
